{
  "name": "mallbook",
  "version": "1.0.0",
  "description": "Shopping Mall Booking System - Full Stack Application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd backend && npm run dev",
    "client:dev": "cd frontend && npm start",
    "server:start": "cd backend && npm start",
    "client:build": "cd frontend && npm run build",
    "seed": "cd backend && npm run seed",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "mall",
    "booking",
    "react",
    "express",
    "mongodb"
  ],
  "author": "Saurabh Chaurasia",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
} 